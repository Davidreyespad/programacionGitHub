Algoritmo Ejercicio7
	
	Escribir "Dime un numero"
    Leer x
    suma <- 0
    Mientras x >= 0 Hacer
        suma <- suma + x
        Escribir "Hasta ahora, la suma es ", suma
        Escribir "Dime otro numero"
        Leer x
    FinMientras
	
    Escribir "Terminado"
	Escribir suma
	
FinAlgoritmo
